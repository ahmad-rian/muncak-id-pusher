{
    "name": "websocket-performance-testing",
    "version": "1.0.0",
    "description": "Performance testing untuk WebSocket Live Cam - Pusher vs Laravel Reverb",
    "main": "test-processor.js",
    "scripts": {
        "test:pusher": "artillery run artillery-pusher-enhanced.yaml",
        "test:pusher:720p": "artillery run artillery-pusher-enhanced.yaml --variables '{\"videoResolution\": \"720p\"}'",
        "test:pusher:1080p": "artillery run artillery-pusher-enhanced.yaml --variables '{\"videoResolution\": \"1080p\"}'",
        "test:pusher:full": "bash run-full-test.sh",
        "test:reverb": "artillery run artillery-reverb-enhanced.yaml",
        "test:reverb:720p": "artillery run artillery-reverb-enhanced.yaml --variables '{\"videoResolution\": \"720p\"}'",
        "test:reverb:1080p": "artillery run artillery-reverb-enhanced.yaml --variables '{\"videoResolution\": \"1080p\"}'",
        "test:reverb:full": "bash run-full-test-reverb.sh",
        "test:all": "npm run test:pusher:full && sleep 300 && npm run test:reverb:full",
        "report:html": "artillery report results/pusher-*.json --output results/pusher-report.html",
        "report:pdf": "node generate-pdf-report.js",
        "analyze": "node analyze-results.js",
        "clean": "rm -rf results/*.json results/*.html results/*.pdf results/*.log"
    },
    "keywords": [
        "artillery",
        "websocket",
        "performance",
        "load-testing",
        "pusher",
        "laravel-reverb",
        "live-streaming"
    ],
    "author": "Ahmad Rian Syaifullah Ritonga",
    "license": "MIT",
    "dependencies": {
        "artillery": "^2.0.16",
        "pusher-js": "^8.4.0",
        "pdfkit": "^0.14.0",
        "ws": "^8.16.0",
        "axios": "^1.6.7"
    },
    "devDependencies": {
        "chart.js": "^4.4.1",
        "chartjs-node-canvas": "^4.1.6"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}